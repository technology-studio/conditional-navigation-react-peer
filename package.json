{
  "name": "@txo-peer-dep/conditional-navigation-react",
  "version": "8.0.2",
  "description": "Technology Studio - Conditional navigation react peer",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/technology-studio/conditional-navigation-react-peer.git"
  },
  "author": {
    "name": "Technology Studio",
    "email": "npm@technologystudio.sk",
    "url": "https://github.com/technology-studio/"
  },
  "license": "UNLICENSED",
  "private": false,
  "scripts": {
    "build": "yarn build:clean && yarn build:lib",
    "build:clean": "yarn rimraf lib",
    "build:lib": "yarn tsc",
    "build:watch": "yarn tsc --watch",
    "test": "jest",
    "test:watch": "concurrently \"yarn build:watch\" \"jest --watch\"",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "compare-boilerplate-version": "./scripts/compare-boilerplate-version.sh",
    "lint": "eslint --max-warnings 0 .",
    "fixcode": "eslint . --fix",
    "git-hook": "yarn -s lint-staged",
    "prepare": "husky && yarn build",
    "print-txo-packages": "yarn list 2> /dev/null|grep @txo|sed 's/.*\\(@txo[^@]*\\)@^*\\([^ ]*\\).*/\\1@\\2/g'|sort|uniq",
    "sanity": "yarn lint:ci && yarn build && tsc --noEmit && yarn test --coverage && yarn compare-boilerplate-version && echo 'success'",
    "semantic-release": "semantic-release",
    "update-boilerplate-version": "./scripts/update-boilerplate-version.sh",
    "lint:ci": "yarn lint",
    "type-check": "tsc --noEmit"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@txo/config-manager": "^3.2.0",
    "@txo/functional": "^4.6.19",
    "@txo/log": "^2.0.16",
    "@txo/types": "^1.7.0"
  },
  "peerDependencies": {
    "@react-navigation/devtools": "^6.0.27",
    "@react-navigation/native": "^6.1.18",
    "@txo-peer-dep/log": "^4.0.4",
    "react-native": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@react-navigation/devtools": "^6.0.27",
    "@react-navigation/native": "^6.1.18",
    "@txo-peer-dep/log": "^4.0.4",
    "@txo/commitlint": "^1.0.18",
    "@txo/log-console": "^3.0.0",
    "@txo/semantic-release": "^2.0.8",
    "@txo/tsconfig": "^1.1.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.0",
    "@types/react-native": "^0.73.0",
    "concurrently": "^9.0.1",
    "eslint-config-txo-typescript-react": "^2.0.73",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "npm-check-updates": "^17.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "0.75.3",
    "redux-saga": "^1.3.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2"
  }
}
